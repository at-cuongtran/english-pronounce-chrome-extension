{
  "manifest_version": 2,
  "name": "English pronounce & dictionary",
  "description": "English pronounce & dictionary",
  "version": "1.2.0",
  "author": "Cuong Tran",
  "background": {
    "scripts": [
      "common.js",
      "main.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon/icon.png",
      "32": "icon/icon.png",
      "48": "icon/icon.png",
      "128": "icon/icon.png"
    },
    "default_title": "English pronounce & dictionary",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/icon.png",
    "32": "icon/icon.png",
    "48": "icon/icon.png",
    "128": "icon/icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent"
  ],
  "commands": {
    "speak-howjsay": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Alt+Shift+J"
      },
      "description": "pronounce using howjsay"
    },
    "speak-ox_us": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Alt+Shift+S"
      },
      "description": "pronounce using Oxford US"
    },
    "speak-ox_uk": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Alt+Shift+K"
      },
      "description": "pronounce using Oxford UK"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  }
}
